generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id            Int      @id @default(autoincrement())
  name          String
  fabricType    String
  gsm           Int
  color         String
  pricePerMeter Float
  stock         Int
  imageUrl      String
  createdAt     DateTime @default(now())
}